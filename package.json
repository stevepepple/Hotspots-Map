{
  "name": "hotspots-server",
  "version": "0.0.1",
  "scripts": {
    "start": "node server.js",
    "both": "\"node src/lib/server.js\" \"react-scripts start\"",
    "start:dev": "concurrently \"nodemon src/lib/server.js\" \"react-scripts start\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.14",
    "path": "^0.12.7",
    "yarn": "^1.12.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "10.x"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
